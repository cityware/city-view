{
    "name": "cityware/city-view",
    "description": "Cityware View",
    "license": "GPL-3.0",
    "keywords": [
        "cityware",
        "view"
    ],
    "homepage": "https://github.com/cityware/city-view",
    "autoload": {
        "psr-4": {
            "Cityware\\View\\": "src/"
        }
    },
    "require": {
        "php": ">=5.3.23"
    },
    "autoload": {
      "classmap": [
        "src/"
      ]
    },
    "require-dev": {
        "zendframework/zend-servicemanager": "~2.5",
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "~4.0"
    },
    "suggest": {
        "ext-intl": "Required for most features of Zend\\I18n; included in default builds of PHP",
        "zendframework/zend-View": "Zend\\View component"
    },
    "minimum-stability":"dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CitywareTest\\View\\": "test/"
        }
    }
}